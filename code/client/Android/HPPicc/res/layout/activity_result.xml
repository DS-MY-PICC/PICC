<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context="${packageName}.${activityClass}" >

    <TextView
        android:id="@+id/tvResult"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="15dp"
        android:fontFamily="HPFutura Book"
        android:text="Image Print Details:"
        android:textColor="#FFFFFF"
        android:textSize="20dp" />

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvResult"
        android:layout_centerHorizontal="true" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="240dp"
            android:layout_height="180dp"
            android:layout_marginTop="30dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="40dp"
            android:background="#FFFFFF" />

        <ImageView
            android:id="@+id/ivResultImage"
            android:layout_width="230dp"
            android:layout_height="155dp"
            android:layout_centerVertical="true"
            android:background="#0096d6"
            android:layout_alignLeft="@+id/imageView1"
            android:layout_marginLeft="5dp"
            android:src="@drawable/focanonmg8220" />
        
        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@+id/imageView1"
            android:layout_alignBottom="@+id/imageView1"
            android:layout_marginBottom="15dp">

            <TableRow
                android:id="@+id/tableRow4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <ImageView
			        android:id="@+id/imageView2"
			        android:layout_width="15dp"
			        android:layout_height="15dp"
			        android:src="@drawable/cyan" />
                <TextView
			        android:id="@+id/tvCyan"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:fontFamily="HPFutura Book"
			        android:text=""
			        android:textColor="#FF00FF"
			        android:textSize="15dp"
			        android:textStyle="italic" />
            </TableRow>

            <TableRow
                android:id="@+id/tableRow11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <ImageView
			        android:id="@+id/imageView3"
			        android:layout_width="15dp"
			        android:layout_height="15dp"
			        android:src="@drawable/magenta" />
                <TextView
			        android:id="@+id/tvMagenta"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:fontFamily="HPFutura Book"
			        android:text=""
			        android:textColor="#00FFFF"
			        android:textSize="15dp" 
			        android:textStyle="italic"/>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow12"
                android:background="#000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <ImageView
			        android:id="@+id/imageView4"
			        android:layout_width="15dp"
			        android:layout_height="15dp"
			        android:src="@drawable/yellow" />
                <TextView
			        android:id="@+id/tvYellow"
			        android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:fontFamily="HPFutura Book"
		            android:text=""
		            android:textSize="15dp"
			        android:textColor="#FFFF00" 
			        android:textStyle="italic"/>
            </TableRow>

            <TableRow
                android:id="@+id/tableRow13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >
                <ImageView
			        android:id="@+id/imageView5"
			        android:layout_width="15dp"
			        android:layout_height="15dp"
			        android:src="@drawable/black" />
                <TextView
		            android:id="@+id/tvBlack"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:fontFamily="HPFutura Book"
		            android:text=""
		            android:textSize="15dp"
		            android:textColor="#BDBDBD"
		            android:textStyle="italic"
		             />
            </TableRow>
        </TableLayout>
        
    </RelativeLayout>

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageView6"
        android:layout_below="@+id/relativeLayout1"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="10dp" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical" >

            <TableLayout
                android:id="@+id/tableLayout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TableRow
                    android:id="@+id/tableRow1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Page Size"
                        android:textColor="#FFFFFF" />

                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text=":"
                        android:textColor="#FFFFFF" />

                    <TextView
                        android:id="@+id/tvRsPageSize"
                        android:layout_marginLeft="10dp"
                        android:text="A4"
                        android:textColor="#0096d6" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Image Resolution"
                        android:textColor="#FFFFFF" />

                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text=":"
                        android:textColor="#FFFFFF" />

                    <TextView
                        android:id="@+id/tvRsImageResolution"
                        android:layout_marginLeft="10dp"
                        android:text="100 DPI"
                        android:textColor="#0096d6" />
                </TableRow>

                <TableRow
                    android:id="@+id/tableRow3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" >

                    <TextView
                        android:text="Print Mode"
                        android:textColor="#FFFFFF" />

                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text=":"
                        android:textColor="#FFFFFF" />

                    <TextView
                        android:id="@+id/tvRsPrintMode"
                        android:layout_marginLeft="10dp"
                        android:text="Grayscale"
                        android:textColor="#0096d6" />
                </TableRow>

                <TableRow
                    android:id="@+id/trVolume"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="15dp"
                    android:onClick="calculatePrintCost" >

                    <TextView
                        android:text="Print Volume"
                        android:textColor="#FFFFFF"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text=":"
                        android:textColor="#FFFFFF"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tvRsPrintVolume"
                        android:layout_marginLeft="10dp"
                        android:text="100 pcs"
                        android:textColor="#0096d6"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <ImageButton
                        android:id="@+id/ivVolome"
                        android:layout_width="20dp"
                        android:layout_height="wrap_content"
                        android:background="#000000"
                        android:onClick="calculatePrintCost"
                        android:padding="0dp"
                        android:src="@drawable/downarrow" />
                </TableRow>

                <TableRow
                    android:id="@+id/trVolumeSeekBar"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:layout_marginRight="15dp"
                    android:visibility="gone" >

                    <SeekBar
                        android:id="@+id/sbVolume"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_span="4"
                        android:max="100"
                        android:progress="1" />
                </TableRow>

                <TableRow
                    android:id="@+id/trPeriod"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onClick="calculatePrintCost"
                    android:layout_marginTop="5dp" >

                    <TextView
                        android:text="Print Period"
                        android:textColor="#FFFFFF"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_marginLeft="10dp"
                        android:text=":"
                        android:textColor="#FFFFFF"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/tvRsPrintPeriod"
                        android:layout_marginLeft="10dp"
                        android:text="10 days"
                        android:textColor="#0096d6"
                        android:textSize="20dp"
                        android:textStyle="bold" />

                    <ImageButton
                        android:id="@+id/ivPeriod"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="#000000"
                        android:onClick="calculatePrintCost"
                        android:padding="0dp"
                        android:src="@drawable/downarrow" />
                </TableRow>

                <TableRow
                    android:id="@+id/trPeriodSeekBar"
                    android:layout_width="wrap_content"
                    android:layout_height="20dp"
                    android:visibility="gone" >

                    <SeekBar
                        android:id="@+id/sbPeriod"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_span="4"
                        android:max="100"
                        android:progress="1" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
    </ScrollView>

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_above="@+id/tlCost"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:background="#FFFFFF" />

    <ImageView
        android:id="@+id/ivRsSelectedImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="5dp"
        android:src="@drawable/oofficejet6600" />

    <TableLayout
        android:id="@+id/tlCost"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton1"
        android:layout_marginLeft="5dp"
        android:layout_alignParentRight="true"
        android:layout_toRightOf="@+id/textView1" >

        <TableRow
            android:id="@+id/tableRow6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:text="Name"
                android:textColor="#FFFFFF" />

            <TextView
                android:layout_marginLeft="5dp"
                android:text=":"
                android:textColor="#FFFFFF" />

            <TextView
                android:scrollHorizontally="true"
                android:id="@+id/tvRsName"
                android:layout_width="match_parent"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textColor="#0096d6" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:text="Price"
                android:textColor="#FFFFFF" />

            <TextView
                android:layout_marginLeft="5dp"
                android:text=":"
                android:textColor="#FFFFFF" />

            <TextView
                android:id="@+id/tvRsPrice"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textColor="#0096d6" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:text="ICPP"
                android:textColor="#FFFFFF" />

            <TextView
                android:layout_marginLeft="5dp"
                android:text=":"
                android:textColor="#FFFFFF" />

            <TextView
                android:id="@+id/tvRsIcpp"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textColor="#0096d6" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:text="TPC"
                android:textColor="#FFFFFF" />

            <TextView
                android:layout_marginLeft="5dp"
                android:text=":"
                android:textColor="#FFFFFF" />

            <TextView
                android:id="@+id/tvRsTPC"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textColor="#0096d6" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >

            <TextView
                android:text="TCO"
                android:textColor="#FFFFFF" />

            <TextView
                android:layout_marginLeft="5dp"
                android:text=":"
                android:textColor="#FFFFFF" />

            <TextView
                android:id="@+id/tvRsTco"
                android:layout_marginLeft="5dp"
                android:text=""
                android:textColor="#0096d6"
                android:textSize="20dp"
                android:textStyle="bold" />
        </TableRow>
    </TableLayout>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/tlCost"
        android:layout_marginLeft="15dp"
        android:layout_below="@+id/imageView6"
        android:fontFamily="HPFutura Book"
        android:text="Print Cost For:"
        android:textColor="#FFFFFF" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="15dp"
        android:background="#000000"
        android:onClick="onClickComparePrinters"
        android:src="@drawable/compareprinters" />
        
</RelativeLayout>